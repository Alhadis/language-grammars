".source.abnf":
	editor:
		commentStart: "; "

".source.bnf":
	editor:
		commentStart: "; "

".source.ebnf":
	editor:
		commentStart: "(*"
		commentEnd:   "*)"
		increaseIndentPattern: "=\\s*$"

".source.lark":
	editor:
		commentStart: "// "

".source.lex":
	editor:
		commentStart: "/*"
		commentEnd:   "*/"
		increaseIndentPattern: "{\\s*$"
